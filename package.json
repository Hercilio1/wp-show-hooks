{
  "name": "wp-show-hooks",
  "description": "Show all triggered hooks fired on the current WordPress page.",
  "author": "Herc√≠lio M. Ortiz",
  "keywords": [
    "WordPress",
    "Plugin"
  ],
  "files": [
    "assets/dev/**/*"
  ],
  "homepage": "https://github.com/Hercilio1/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hercilio1/wp-show-hooks"
  },
  "scripts": {
    "js:dev": "webpack --config assets/dev/js/webpack.config.js --watch",
    "js:build": "webpack --config assets/dev/js/webpack.prod.js",
    "js:check": "eslint 'assets/dev/js/**/*.js'",
    "js:fix": "eslint 'assets/dev/js/**/*.js' --fix",
    "sass:dev": "webpack --config assets/dev/sass/webpack.config.js --watch",
    "sass:build": "webpack --config assets/dev/sass/webpack.prod.js ",
